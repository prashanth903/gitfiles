<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   
    xmlns:aop="http://www.springframework.org/schema/aop"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd   
    http://www.springframework.org/schema/aop   
    http://www.springframework.org/schema/aop/spring-aop-3.0.xsd ">
    
    
    <bean name="triangle" class="com.org.aop.Triangle">
    <property name="name" value="Triangle1 name"></property></bean>

<bean name="circle" class="com.org.aop.Circle">
    <property name="name" value="circle1 name"></property></bean>
    
    <bean name="shapeservice" class="com.org.service.ShapeService" autowire="byName"/>
    
    <bean name="loggingAspect" class="com.org.aspect.LoggingAspect"></bean>
    
    </beans>